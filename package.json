{
  "name": "pluginable",
  "version": "1.1.0",
  "description": "Another plugin manager!",
  "main": "index.js",
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:confuser/node-pluginable.git"
  },
  "keywords": [
    "plugin",
    "module",
    "modular",
    "component",
    "auto"
  ],
  "author": "James Mortemore <jamesmortemore@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/confuser/node-pluginable/issues"
  },
  "dependencies": {
    "array-duplicated": "^1.0.0",
    "async": "^1.5.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "0.3.5",
    "jscs": "2.5.0",
    "jshint": "2",
    "jshint-full-path": "1.1.1",
    "mocha": "2"
  }
}
